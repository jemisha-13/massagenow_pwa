        NgxStarsComponent.prototype.clickedInFirstHalf = /**
         * @param {?} event
         * @return {?}
         */
            function (event) {
                /** @type {?} */
                var starIcon = /** @type {?} */ (event.target);
                return event.pageX < starIcon.getBoundingClientRect().left + starIcon.offsetWidth / 2;
            };
